public class Main {
    public static void main(String[] args) {
                System.out.println("Numeros del 1 al 100 divisibles por 2 y 3:");

                for (int i = 1; i <= 100; i++) {
                    if (i % 6 == 0) { // Divisibles por 2 y 3 = divisibles por 6
                        System.out.print(i + " ");
                    }
                }
            }
        }
